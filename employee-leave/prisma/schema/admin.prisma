enum Gender {
  MALE
  FEMALE
}

model Admin {
  id            String   @id @default(cuid())
  namaDepan     String
  namaBelakang  String
  email         String   @unique
  tanggalLahir  DateTime
  jenisKelamin  Gender
  password      String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@map("admins")
}
